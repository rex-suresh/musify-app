{
  "name": "musify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:staging": "react-native run-android --mode=stagingdebug",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "buildAPK": "ENVFILE=.env.prod && cd ./android/ && ./gradlew build && cd .. && ./bin/cpApk.sh",
    "android": "ENVFILE=.env.staging && react-native run-android",
    "ios": "ENVFILE=.env.staging && react-native run-ios",
    "start": "ENVFILE=.env.staging && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.3.4",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-config": "^1.5.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-paper": "^5.4.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-track-player": "^4.0.0-rc01",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.71.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.76.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
